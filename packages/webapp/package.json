{
  "name": "webapp",
  "version": "1.0.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn install && react-scripts build",
    "test": "react-scripts test",
    "test:e2e": "./node_modules/.bin/cypress open",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "axios": "1.6.0",
    "internationalization": "0.1.2",
    "localforage-startswith": "1.4.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.1",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.1.1",
    "redux": "4.0.4",
    "shared-store": "1.0.0",
    "styled-components": "4.3.2"
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/config/*.{js,jsx,ts,tsx}",
      "!src/routes/*.{js,jsx,ts,tsx}",
      "!src/store/*.{js,jsx,ts,tsx}",
      "!src/**/*-wrappers.{js,jsx,ts,tsx}",
      "!src/**/*-wrapper.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!<rootDir>/src/screens/**",
      "!<rootDir>/src/components/icons/*",
      "!<rootDir>/src/App.js",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/serviceWorker.js"
    ]
  }
}
